<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <OutputType>Library</OutputType>
    <Title>OSRS.Dotnet.Tools</Title>
    <Authors>JustinRidings</Authors>
    <Version>1.0.4</Version>
    <Description>A library of tools for interacting with the game Oldschool Runescape's (OSRS) API's using .NET</Description>
    <RepositoryUrl>https://github.com/JustinRidings/OSRS.Dotnet.Tools</RepositoryUrl>
    <PackageTags>OSRS;Oldschool Runescape;Runescape</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

</Project>
